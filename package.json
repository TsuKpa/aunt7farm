{
  "name": "aunt7sfarm",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node server.js",
    "postinstall": "ng build --output-path aunt7sfarm --prod",
    "preinstall": "npm install -g @angular/cli@latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A .",
      "pre-push": "npm run lint && git branch | grep \"*\" | egrep -v \"^* master$\""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
