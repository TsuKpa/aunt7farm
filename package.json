    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@ngneat/tailwind": "^6.0.3",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "express": "^4.17.1",
    "postcss": "^7.0.35",
    "rxjs": "~6.6.0",
